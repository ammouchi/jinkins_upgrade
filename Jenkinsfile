pipeline{
	agent  any
	stages {
		stage('Init'){
			steps {
				echo  'Hi This is Mohamed '
				echo 'we are starting tests'
			}

		}
		stage('Build'){
			steps {
				echo  'Building'
				
			}

		}
		stage('Deploy'){
			steps {
				echo  'Deploying'
				
			}

		}
		stage('DeployToProd'){
			steps {
				echo  'Deploying To Prod'
				
			}

		}
	}
}